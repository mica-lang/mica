# Tests if the GC can handle a function getting deleted during a call.

struct LinkedList

impl LinkedList
   func new(value, next) constructor
      @value = value
      @next = next
   end
end

func make_long_list()
   i = 0
   list = nil
   # Allocate enough objects to cause a collection to happen.
   while i < 1600 do
      list = LinkedList.new(i, list)
      i = i + 1
   end
   list
end

func hi()
   hi = nil
   make_long_list()
   nil
end

hi()
